{
  "manifest_version": 2,

  "name": "Favourite Audio",
  "description": "Add your favourites www.myinstants.com audios",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Favourite Audio"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"

  ]
}
